{
    "name": "signifly/travy-schema",
    "description": "Schema for Travy",
    "homepage": "https://github.com/signifly/travy-schema",
    "license": "MIT",
    "authors": [
        {
            "name": "Morten Poul Jensen",
            "email": "mpj@signifly.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "ext-json": "*",
        "illuminate/http": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Signifly\\Travy\\Schema\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Signifly\\Travy\\Schema\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {}
}
